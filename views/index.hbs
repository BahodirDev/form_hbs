<div class="p-4">
    {{#each title}}
    <div class="card mt-2">
        <div class="card-body d-flex justify-content-between ">
            <h5 class="card-title">
                {{name}}
            </h5>
            <div>
                <button class="btn btn-outline-warning">
                     <a href="/edit/{{id}}">EDIT</a>
                </button>
                <button class="btn btn-outline-danger">
                    <a href="/delete/{{id}}">DELETE</a>
                </button>
            </div>
        </div>
    </div>
    {{/each}}
</div>
